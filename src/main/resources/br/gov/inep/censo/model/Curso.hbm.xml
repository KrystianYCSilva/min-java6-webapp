<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.gov.inep.censo.model.Curso" table="curso" dynamic-update="true">
        <id name="id" column="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="codigoCursoEmec" column="codigo_curso_emec" type="string" not-null="true" length="12"/>
        <property name="nome" column="nome" type="string" not-null="true" length="160"/>
        <property name="nivelAcademico" column="nivel_academico" type="string" not-null="true" length="30"/>
        <property name="formatoOferta" column="formato_oferta" type="string" not-null="true" length="20"/>
        <property name="cursoTeveAlunoVinculado" column="curso_teve_aluno_vinculado" type="integer" not-null="true"/>
    </class>
</hibernate-mapping>
