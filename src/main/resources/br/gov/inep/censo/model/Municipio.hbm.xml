<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.gov.inep.censo.model.Municipio" table="municipio" mutable="false">
        <id name="codigo" column="codigo" type="string" length="7"/>
        <property name="nome" column="nome" type="string" not-null="true" length="180"/>
        <property name="codigoUf" column="codigo_uf" type="integer" not-null="true"/>
        <property name="nomeUf" column="nome_uf" type="string" not-null="true" length="80"/>
    </class>
</hibernate-mapping>
