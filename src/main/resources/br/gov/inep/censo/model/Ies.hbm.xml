<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.gov.inep.censo.model.Ies" table="ies" dynamic-update="true">
        <id name="id" column="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="idIesInep" column="id_ies_inep" type="long"/>
        <property name="nomeLaboratorio" column="nome_laboratorio" type="string" not-null="true" length="200"/>
        <property name="registroLaboratorioIes" column="registro_laboratorio_ies" type="string" length="14"/>
        <property name="laboratorioAtivoAno" column="laboratorio_ativo_ano" type="integer"/>
        <property name="descricaoAtividades" column="descricao_atividades" type="string" length="2000"/>
        <property name="palavrasChave" column="palavras_chave" type="string" length="200"/>
        <property name="laboratorioInformatica" column="laboratorio_informatica" type="integer"/>
        <property name="tipoLaboratorio" column="tipo_laboratorio" type="integer"/>
        <property name="codigoUfLaboratorio" column="codigo_uf_laboratorio" type="integer"/>
        <property name="codigoMunicipioLaboratorio" column="codigo_municipio_laboratorio" type="string" length="7"/>
    </class>
</hibernate-mapping>
