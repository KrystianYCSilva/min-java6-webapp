<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.gov.inep.censo.model.LayoutCampo" table="layout_campo" mutable="false">
        <id name="id" column="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="modulo" column="modulo" type="string" not-null="true" length="30"/>
        <property name="numeroCampo" column="numero_campo" type="integer" not-null="true"/>
        <property name="nomeCampo" column="nome_campo" type="string" not-null="true" length="255"/>
        <property name="obrigatoriedade" column="obrigatoriedade" type="string" length="20"/>
    </class>
</hibernate-mapping>
